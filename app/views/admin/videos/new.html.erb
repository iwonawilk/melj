<div class="form container my-5">
  <%= simple_form_for(@video, url: admin_videos_path) do |f| %>
    <%= f.input :title %>
    <%= f.input :url %>
    <div class="d-flex">
      <%= f.submit "Add Video", class: "btn btn-success w-50" %>
      <%= link_to "Back", admin_videos_path, class: "btn btn-primary w-50" %>
    </div>
  <% end %>
</div>
